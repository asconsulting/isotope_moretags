{
    "name":"asconsulting/isotope_moretags",
    "description":"Module that adds additional insert tags for Isotope eCommerce products in the Contao CMS.",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "isotope", "inserttags", "tags", "sku", "details", "attributes"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/core-bundle": "~4.13",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "IsotopeAsc\\": [
                "system/modules/isotope_moretags/library/IsotopeAsc"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_moretags": "system/modules/isotope_moretags"
            }
        }
    },
    "minimum-stability": "dev"
}